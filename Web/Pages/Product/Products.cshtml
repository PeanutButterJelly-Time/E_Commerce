@page
@model Web.Pages.Product.ProductsModel
@{Layout = "_Layout";
    }
    
<div>
    @foreach (var product in Model.List)
    {
        <section class="card text-center float-left col-5">
            <h5>@product.Name</h5>
            <div>
                <p>Manufacturer: @product.Manufacturer</p>

            </div>
        </section>
    }
    </div>
